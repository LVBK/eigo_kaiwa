<div class="section">
      <div class="container">
        <div class="row">
<%= form_for(@user_log) do |f| %>
  <% if @user_log.errors.any? %>
    <div class="col-md-6 col-md-offset-3" id="error_explanation">
      <h2><%= pluralize(@user_log.errors.count, "error") %> prohibited this user_log from being saved:</h2>

      <ul>
      <% @user_log.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
          <div class="col-md-6 col-md-offset-3 white-form" style='padding:30px'>
            <form role="form">

              <div class="form-group has-feedback">
                <label class="control-label" for="exampleInputEmail1">Email address</label>
                <input class="form-control" id="exampleInputEmail1"
                type="text" disabled="disabled" value="<%= current_user.email %>">
              </div>
              <div class="form-group has-feedback">
                <label class="control-label" for="exampleInputPassword1">Sentence learning</label><br>
                <%= f.number_field :sentence_learning %>
              </div>
              <div class="form-group has-feedback">
                <label class="control-label">Create at</label><br>
                <%= f.datetime_select :created_at %>
              </div>
              <div class="actions">
                <%= f.submit nil, :class => 'btn btn-primary' %>
              </div>
            </form>
<% end %>

          </div>
        </div>
      </div>
    </div>